{
	"tls": {
		"kind": "attr",
		"desc": "Требовать TLS",
		"enum": {
			"on":  "Требовать TLS",
			"off": "Не требовать TLS"
		},
		"default" : "on"
	},

	"group": {
		"kind": "attr",
		"desc": "Имя группы или идентификатор туннеля"
	},

	"user": {
		"kind": "attr",
		"desc": "Имя пользователя или идентификатор"
	},

	"key-format": {
		"kind": "attr",
		"desc": "Формат ключа",
		"enum": {
			"base64":	"Кодировка передачи Base64, RFC 2045",
			"hex-dump":	"Шестнадцатеричный дамп",
			"hex-number":	"Шестнадцатеричное число",
			"plain":	"Строка печатных символов"
		},
		"default": "plain"
	},

	"key": {
		"kind": "attr",
		"desc": "Пароль, ключ или ключевая фраза"
	},

	"cert": {
		"kind": "attr",
		"desc": "Имя X.509 сертификата",
		"ref": ["pki", "ca", "*", "certificate"]
	}
}
